{
  "family": "tablebar-app-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::816122495557:role/ecs-tablebar-api-task-execution-role",
  "taskRoleArn": "arn:aws:iam::816122495557:role/tablebar-api-ecs-role",
  "containerDefinitions": [
    {
      "name": "tablebar-api",
      "image": "123456789012.dkr.ecr.us-east-1.amazonaws.com/myapp:placeholder",
      "portMappings": [{ "containerPort": 3000, "protocol": "tcp" }],
      "essential": true
    }
  ]
}
